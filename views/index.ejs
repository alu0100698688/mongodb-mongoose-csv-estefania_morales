
<form action="/csv" method="post">
  <div class="contenido_total">
    <div class="titulo_practica">
      <h1 align="center">
        <font color="blue">Comma Separated Values (CSV) Analyzer with Ajax</font>
      </h1>
      <hr>
    </div>
    <div class="contenido">
      <table class="center" id="input">
        <tr><th>Escriba aqu√≠ una entrada CSV o bien</th></tr>
        <tr><th>Arrastre un fichero CSV hasta la zona de entrada</th></tr>
        <tr>
          <th>
            <textarea class = "drop_zone" autofocus="" cols = "80" rows = "5"  name="csv" id="original"></textarea>
          </th>
          <th>
            <button type="submit" id="parse"> Analizar!</button>
          </th>
        </tr>
        <tr>
        </tr>
        <tr>
          <th>
            <div class="example">
              Click para cargar ejemplos:
              <button class ="example" type="button" id="in1"></button>
              <button class ="example" type="button" id="in2"></button>
              <button class ="example" type="button" id="in3"></button>
              <button class ="example" type="button" id="in4"></button>
            </div>
          </th>
        </tr>
        <tr>
          <th>
              Cargar archivo:
              <input type="file" class="inputfile" />
          </th>
        </tr>
        <tr>
          <th>
              Guardar una entrada:
              <input type="text" id="nameEnt" pattern="^[a-zA-Z_]\w*\.csv$">
              <button type= "button" id="save">Guardar</button>
          </th>
        </tr>
      </table>

      <div class="output" id="finaltable">
      </div>
    </div>
  </div>
</form>
